<h1>Lists</h1>

<%# <%= render @list %>
<ul class="list-group">
  <% @list.movies.each do |movie| %>
    <li class="list-group-item">
      <%= movie.title %>
      <%= movie.overview %>
      <%= movie.poster_url %>
      <%= movie.rating %>
    </li>
  <% end %>
</ul>

<div>
  <%= link_to "Back to Movies List", lists_path %>
</div>
